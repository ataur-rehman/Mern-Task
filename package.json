{
    "name": "timesheet-management",
    "version": "1.0.0",
    "description": "MERN stack timesheet management application",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "cd timesheet-backend && npm run dev",
        "dev:frontend": "cd timesheet-frontend && npm start",
        "build": "cd timesheet-frontend && npm run build",
        "install:all": "npm install && cd timesheet-backend && npm install && cd ../timesheet-frontend && npm install",
        "start:backend": "cd timesheet-backend && npm start",
        "start:frontend": "cd timesheet-frontend && npm start",
        "test": "cd timesheet-frontend && npm test",
        "clean": "rm -rf timesheet-backend/node_modules timesheet-frontend/node_modules node_modules",
        "setup": "npm run install:all && npm run setup:env",
        "setup:env": "cd timesheet-backend && cp .env.example .env && cd ../timesheet-frontend && cp .env.example .env"
    },
    "devDependencies": {
        "concurrently": "^7.6.0"
    },
    "author": "",
    "license": "MIT",
    "keywords": [
        "timesheet",
        "mern",
        "mongodb",
        "express",
        "react",
        "nodejs",
        "typescript",
        "material-ui"
    ]
}